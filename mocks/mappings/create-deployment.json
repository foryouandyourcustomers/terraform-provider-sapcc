{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPathPattern": "/([a-zA-Z0-9]*)/deployments",
    "bodyPatterns": [
      {
        "equalToJson": {
          "buildCode": "000000.0",
          "databaseUpdateMode": "NONE",
          "environmentCode": "d0",
          "strategy": "ROLLING_UPDATE"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "code": "000000"
    },
    "transformers": [
      "response-template"
    ],
    "delayDistribution": {
      "type": "lognormal",
      "median": 95,
      "sigma": 0.95
    }
  }
}