{
  "priority":1,
  "request": {
    "method": "POST",
    "urlPathPattern": "/([a-zA-Z0-9]*)/builds",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.applicationCode",
          "contains": "commerce-cloud"
        },
        "matchesJsonPath": {
          "expression": "$.branch",
          "contains": "main"
        },
        "matchesJsonPath": {
          "expression": "$.name",
          "contains": "ci-build-0"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "code": "000000.0"
    },
    "delayDistribution": {
      "type": "lognormal",
      "median": 95,
      "sigma": 0.95
    }
  }
}